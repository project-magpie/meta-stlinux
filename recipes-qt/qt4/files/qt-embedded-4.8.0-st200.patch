+	asm volatile   ("	call $r63 = 1f 		\n"
+			"	;; 			\n"
+	 		"1: 	ldw %0 = 0[%2] 		\n"
+	 		"	mov $r62 = $r63 	\n"
+	 		"	or $r12 = $r12, 1 	\n"
+	 		"	;; 			\n"
+	 		"	add %1  = %0, %3 	\n"
+	 		"	;; 			\n"
+	 		"	stw 0[%2] = %1 		\n"
+		 	"	and $r12 = $r12, ~1 	\n"
+		 	: "=&r" (old) /* %0 */, "=&r" (tmp) /* %1 */
+		 	: "r" (&_q_value) /* %2 */, "r" (valueToAdd) /* %3 */
+		 	: "r62", "r63", "memory"
