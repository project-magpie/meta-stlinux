+inline bool QBasicAtomicInt::testAndSetOrdered(int expectedValue, int newValue)
+{
+	register int tmp, ret;
+
+	asm volatile(	"	call $r63 = 1f 		\n"
+			"	;; 			\n"
+			"1: 	ldw %0 = 0[%2] 		\n" /* tmp = *_q_value) */
+	   		"	mov $r62 = $r63 	\n"
+			"	or $r12 = $r12, 1 	\n"
+			"	;;			\n"
+			"	cmpeq $b7 = %0, %3 	\n"
+	      		"	cmpeq %1 = %0, %3 	\n"
+			"	;; 			\n"
+			"	slct  %0 = $b7, %4, %0 	\n"
+			"	;; 			\n"
+			"	stw 0[%2] = %0		\n"
+			"	and $r12 = $r12, ~1 	\n"
+		 	: "=&r" (tmp) /* %0 */ , "=&r" (ret) /* %1 */
+		 	: "r" (&_q_value) /* %2 */ , "r" (expectedValue) /* %3 */, "r" (newValue) /* %4 */
+                 	: "r62", "r63", "b7", "memory");
+	return ret;
+}
