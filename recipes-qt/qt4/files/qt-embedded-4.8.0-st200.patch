+inline bool QBasicAtomicInt::ref()
+{
+	register int old, tmp;
+
+    	asm volatile(	"	call $r63 = 1f 		\n"
+			"	;; 			\n"
+			"1:	ldw %0 = 0[%2] 		\n" /* old = *_q_value */
+			"    	mov $r62 = $r63 	\n"
+		 	"    	or  $r12 = $r12, 1 	\n"
+			"	;; 			\n"
+		 	"	add %1 = %0, 1 		\n" /* tmp = old + 1 */
+		 	"	;; 			\n"
+		 	"	stw 0[%2] = %1 		\n" /* *_q_value = tmp */
+		 	"	and $r12 = $r12, ~1 	\n"
+                 	: "=&r" (old) /* %0 */ , "=&r" (tmp) /* %1 */
+                 	: "r" (&_q_value) /* %2 */
+                 	: "r62", "r63", "memory");
+
+    return old != -1;
+}
+
+inline bool QBasicAtomicInt::deref()
+{
+    	register int old, tmp;
+
+    	asm volatile(	"	call $r63 = 1f 		\n"
+			"	;; 			\n"
+		 	"1:	ldw %0 = 0[%2] 		\n" /* old = *_q_value */
+		 	"    	mov $r62 = $r63		\n"
+		 	"    	or  $r12 = $r12, 1 	\n"
+		 	"	;; 			\n"
+		 	"	add %1 = %0, -1 	\n" /* tmp = old - 1 */
+		 	"	;; 			\n"
+		 	"	stw 0[%2] = %1 		\n" /* *_q_value = tmp */
+		 	"	and $r12 = $r12, ~1 	\n"
+                 	: "=&r" (old) /* %0 */ , "=&r" (tmp) /* %1 */
+                 	: "r" (&_q_value) /* %2 */
+                 	: "r62", "r63", "memory");
+
+    return old != 1;
+}
