+inline int QBasicAtomicInt::fetchAndStoreOrdered(int newValue)
+{
+	register int old;
+
+	asm volatile("		call $r63 = 1f 		\n"
+		     "		;; 			\n"
+		     "1:	ldw %0 = 0[%1] 		\n" /* prev = *_q_value */
+		     "		mov $r62 = $r63 	\n"
+		     "		or  $r12 = $r12, 1	\n"
+		     "		;; 			\n"
+		     "		stw 0[%1] = %2		\n"
+		     "		and $r12 = $r12, ~1 	\n"
+		     :	"=&r" (old) /* %0 */
+		     :	"r" (&_q_value) /* %1 */ , "r" (newValue) /* %2 */
+		     : "r62", "r63", "memory");
+
+	return old;
+}
